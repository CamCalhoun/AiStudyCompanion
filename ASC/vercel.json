{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "/server/main.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites":  [
    {
      "source": "/api/(.*)",
      "destination": "/server/main.py"
    }
    {
      "source": "/(.*)", 
      "destination": "/"
    }
  ]
}



